<div id="<%= dom_id order %>">
  <p>
    <strong>Total payment:</strong>
    <%= order.total_payment %>
  </p>

  <p>
    <strong>Order date:</strong>
    <%= order.order_date %>
  </p>

  <p>
    <strong>Product:</strong>
    <%= order.product_id %>
